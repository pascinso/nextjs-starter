{
  "name": "nextjs-starter",
  "version": "9.0.0",
  "author": {
    "name": "Ebuka Isiadinso",
    "email": "ebukaa@proton.me"
  },
  "private": true,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": "https://github.com/pascinso/nextjs-starter.git",
  "license": "MIT",
  "proxy": "http://localhost:8900",
  "scripts": {
    "cap:add-android": "npx cap add android",
    "cap:add-electron": "npx cap add electron",
    "cap:add-ios": "npx cap add ios",
    "cap:copy": "npx cap copy",
    "cap:init": "npx cap init",
    "cap:open-android": "npx cap open android",
    "cap open-electron": "npx cap open electron",
    "cap open-ios": "npx cap open ios ",
    "clear:react-import": "npx react-codemod update-react-imports",
    "clear:cache": "npm run remove:node-modules && npm run remove:package-lock && npm run npm:clear-cache",
    "eslint": "eslint --ext .ts,.tsx pages",
    "eslint:fix": "eslint --fix --ext .ts,.tsx pages",
    "next:build": "next build",
    "npm:clear-cache": "npm cache clean --force",
    "next:dev": "next dev -p 8900",
    "next:export": "next export",
    "next:lint": "next lint",
    "next:start": "next start",
    "next:telemetry-disable": "npx next telemetry disable",
    "next:telemetry-enable": "npx next telemetry enable",
    "next:telemetry-status": "npx next telemetry status",
    "npm audit": "npm audit fix",
    "npm:check-updates": "npx npm-check-updates -u",
    "npm:install": "npm i",
    "npm:update": "npm update",
    "npm:upgrade": "sudo npm update npm -g",
    "remove:node-modules": "rm -rf node_modules",
    "remove:package-lock": "rm -f package-lock.json",
    "test": "jest"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/jest": "^29.2.4",
    "@types/node": "18.11.13",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "node-notifier": "^10.0.1",
    "prettier": "^2.8.1",
    "sass": "^1.56.2",
    "typescript": "^4.9.4"
  }
}
